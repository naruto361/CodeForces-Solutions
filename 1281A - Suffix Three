
#include <iostream>

using namespace std;

int main()
{
    int n;
    cin>>n;
    while(n--)
    {
        string s;
        cin>>s;
        char c=s[s.length()-1];
        if(c=='o') cout<<"FILIPINO";
        else if(c=='u') cout<<"JAPANESE";
        else cout<<"KOREAN";
        cout<<endl;
        
    }
}
