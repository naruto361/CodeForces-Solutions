
#include <bits/stdc++.h>

using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        int a[n];
        for(int i=0;i<n;i++) cin>>a[i];
        //reverse(a,a+n);
        for(int i=0;i<n;i=i+2)
        cout<<-a[i+1]<<" "<<a[i]<<" ";
        cout<<endl;
    }
    
}
